function STRIKER_SELECT()
    self.clearButtons()
    self.createButton({click_function = 'STRIKER_NIL', function_owner = self, label = 'None', position = {3, 6, 0},  rotation = {-60, 0, 0}, width=1600, height=400, font_size=250})
    self.createButton({click_function = 'STRIKER_PERFECT', function_owner = self, label = 'Perfect', position = {-3, 6, 0},  rotation = {-60, 0, 0}, width=1600, height=400, font_size=250})
    self.createButton({click_function = 'STRIKER_AILE', function_owner = self, label = 'Aile', position = {0, 5, 0},  rotation = {-60, 0, 0}, width=1600, height=400, font_size=250})
    self.createButton({click_function = 'STRIKER_SWORD', function_owner = self, label = 'Sword', position = {-3, 5, 0},  rotation = {-60, 0, 0}, width=1600, height=400, font_size=250})
    self.createButton({click_function = 'STRIKER_LAUNCHER', function_owner = self, label = 'Launcher', position = {3, 5, 0},  rotation = {-60, 0, 0}, width=1600, height=400, font_size=250})
    self.createButton({click_function = 'STRIKER_NOIR', function_owner = self, label = 'Noir', position = {-3, 4, 0},  rotation = {-60, 0, 0}, width=1600, height=400, font_size=250})
    self.createButton({click_function = 'STRIKER_JET', function_owner = self, label = 'Jet', position = {0, 4, 0},  rotation = {-60, 0, 0}, width=1600, height=400, font_size=250})
    self.createButton({click_function = 'STRIKER_GUNBARREL', function_owner = self, label = 'Gunbarrel', position = {3, 4, 0},  rotation = {-60, 0, 0}, width=1600, height=400, font_size=250})
    self.createButton({click_function = 'createRobot', function_owner = self, label = 'Cancel', position = {0, 3, 0},  rotation = {-60, 0, 0}, width=1200, height=400, font_size=250})
end

