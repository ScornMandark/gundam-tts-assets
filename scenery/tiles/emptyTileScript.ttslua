function onLoad(script_state)
    -- JSON decode our saved state
    local state = JSON.decode(script_state)
    local tile = getObjectFromGUID(state.guids.tile)
    tile.setTag('mapTile')
    return JSON.encode(state)
end

function onSave()
    local state = {
        guids = {
            tile = self.getGUID() -- GUID (a string)
        },
    }
    return JSON.encode(state)
end        