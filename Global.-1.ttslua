--[[ Lua code. See documentation: https://api.tabletopsimulator.com/ --]]

--[[ The onLoad event is called after the game save finishes loading. --]]
function onLoad()
    --[[ print('onLoad!') --]]
    makeTheHaro()
end

function makeTheHaro()

    spawnParams = {
        type = "Custom_Assetbundle",
        position = {x=30, y=10, z=0},
        rotation = {x=0, y=90, z=0},
        scale = {1, 1, 1},
        sound = false,
        snap_to_grid = false,
        callback_function = function(obj) spawn_callback(obj, "Haro") end
    }
    customParams = {
        assetbundle = 'https://github.com/ScornMandark/gundam-tts-assets/blob/main/haro.unity3d?raw=true',
        type = 1, --[[ figurine --]]
        material = 2, --[[ metal --]]
    }

    obj = spawnObject(spawnParams)
    obj.setCustomObject(customParams)
    initializeHaro()
    obj.locked = true
    obj.interactable = false
    scaler = 1
    MSPos = obj.getPosition()
    MSPos = {MSPos.x-5,MSPos.y-5,MSPos.z-5}
end

function initializeHaro()
    obj.clearButtons()
    params = {
        click_function = 'getInTheRobot',
        function_owner = self,
        label = 'Create',
        position = {0, 3, 0},
        rotation = {-60, 0, 0},
        width = 1200,
        height = 400,
        font_size = 340,
        tooltip = "Get a robot!",
    }
    obj.createButton(params)
end

--[[big ass branching MS selector]]

